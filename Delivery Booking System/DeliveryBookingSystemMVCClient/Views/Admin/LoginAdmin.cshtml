@model DeliveryBookingSystemMVCClient.Models.Executive

@{
    ViewData["Title"] = "LoginAdmin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Admin Executive Login</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="LoginAdmin">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ExecutiveId" class="control-label"></label>
                <input asp-for="ExecutiveId" class="form-control" />
                <span asp-validation-for="ExecutiveId" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
               
                @Html.PasswordFor(model => model.Password, new { @class = "form-control" })
                
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
           
           
            <div class="form-group">
                <input type="submit" value="Login" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



