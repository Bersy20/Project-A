@model IEnumerable<DeliveryBookingSystemMVCClient.Models.Executive>

@{
    ViewData["Title"] = "IndexOfExecutive";
      Layout = "~/Views/Shared/AdminExecutiveListLayout.cshtml";
}

<h3>List Of Executives</h3>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExecutiveName)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PinCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsVerified)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExecutiveName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PinCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsVerified)
            </td>
            <td>

                 @Html.ActionLink("Edit", "EditExecutiveDetails", new { id = item.ExecutiveId }) |
             
                @Html.ActionLink("Delete", "DeleteExecutive", new { id = item.ExecutiveId })
            </td>
        </tr>
}
    </tbody>
</table>
